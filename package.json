{
  "name": "presentation-cell",
  "version": "0.1.0",
  "description": "A simple theme, based on JupyterLab light, to show only the current cell.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jpivarski/JupyterLab-presentation-cell",
  "bugs": {
    "url": "https://github.com/jpivarski/JupyterLab-presentation-cell/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jim Pivarski",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jpivarski/JupyterLab-presentation-cell.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
